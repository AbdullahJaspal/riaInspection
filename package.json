{
  "name": "RiaInspections",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "resetCache": "react-native start --reset-cache",
    "iOSDevices": "xcrun simctl list devices",
    "bundle_debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && open app/build/outputs/apk/debug",
    "iPhone8": "react-native run-ios --simulator=\"iPhone 8\"",
    "podInstall": "cd ios && pod install && cd ..",
    "gradlewClean": "cd android && ./gradlew clean && cd ..",
    "assembleRelease": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "bundleRelease": "cd android && ./gradlew bundleRelease && open app/build/",
    "pod_clean_and_install": "cd ios && pod deintegrate  && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-community/picker": "^1.8.1",
    "@react-navigation/drawer": "^5.12.3",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.6",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-animatable": "^1.3.3",
    "react-native-dropdown-picker": "^4.0.2",
    "react-native-elements": "^3.1.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-image-picker": "^3.3.2",
    "react-native-modal": "^11.7.0",
    "react-native-normalize": "^1.0.1",
    "react-native-paper": "^4.7.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-native-shapes": "^0.1.0",
    "react-native-signature-canvas": "^3.6.0",
    "react-native-snackbar": "^2.3.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-vector-icons": "^8.0.0",
    "react-native-webview": "^11.3.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "rn-range-slider": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
